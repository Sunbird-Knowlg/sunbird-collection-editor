  "scripts": {
    "ng": "ng",
    "postinstall": "node -e \"const fs=require('fs'),p='node_modules/@angular-devkit/build-angular/node_modules';!fs.existsSync(p)&&fs.mkdirSync(p,{recursive:true});!fs.existsSync(p+'/@babel')&&fs.symlinkSync('../../../@babel',p+'/@babel',process.platform==='win32'?'junction':'dir')\"",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build collection-editor-library && node assets-copy.js",
    "build-lib:prod": "ng build collection-editor-library --configuration production && node assets-copy.js",
    "test-coverage": "ng test collection-editor-library --code-coverage=true --watch=false --source-map=false --browsers=ChromeHeadless",
    "build-web-component": "npm run build-lib && ng build collection-editor-library-wc && node ./build-wc.js"
  },
